    "next-auth": "^4.24.7",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "@types/qrcode": "^1.5.5",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.25",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
